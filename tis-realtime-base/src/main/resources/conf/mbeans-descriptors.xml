<?xml version="1.0"?>
  <!DOCTYPE mbeans-descriptors PUBLIC
   "-//Apache Software Foundation//DTD Model MBeans Configuration File"
   "http://jakarta.apache.org/commons/dtds/mbeans-descriptors.dtd">

<mbeans-descriptors>
	<!-- className="com.qlangtech.tis.realtime.transfer.TransferStatusMBean" -->
	<mbean name="transferStatus" description="ons listern status"
		domain="Users" group="Group"
		type="com.qlangtech.tis.realtime.transfer.IOnsListenerStatus">

		<attribute name="tableUpdateCount" description="table update count(json)"
			type="java.lang.String" writeable="false" />

		<attribute name="bufferQueueRemainingCapacity" description="buffer Queue Remaining Capacity"
			type="java.lang.Integer" writeable="false" />

		<attribute name="consumeIncreaseCount" description="consume Increase Count"
			type="java.lang.Long" writeable="false" />

		<attribute name="consumeErrorCount" description="consume error Count"
			type="java.lang.Long" writeable="false" />
		<attribute name="ingorRowsCount" description="ingor Rows Count"
			type="java.lang.Long" writeable="false" />
		<operation name="resumeConsume" />
		<operation name="pauseConsume" />
	</mbean>
</mbeans-descriptors>